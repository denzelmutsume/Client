  VerticalScrollBar
    id: scrollBar
    step: 50
    pixels-scroll: true
    anchors.top: itemsPanel.top
    anchors.left: itemsPanel.right
    anchors.bottom: itemsPanel.bottom
    margin-left: 11
    opacity: 0.8

  Item
    id: devoted
    size: 16 18
    image-source: /images/heldsmachine/devotedtoken
    anchors.top: itemsPanel.bottom
    anchors.left: itemsPanel.left
    margin-left: 7
    margin-top: 24
    !tooltip: tr("Devoted Token")
    opacity: 0.9

  UILabel
    id: devotedcount
    image-color: alpha
    font: verdana-11px-rounded
    anchors.right: devoted.left
    anchors.verticalCenter: devoted.verticalCenter
    margin-top: 1
    margin-right: 3
    text-align: right
    color: white
    opacity: 0.9
    text-auto-resize: true

  Item
    id: dungeon
    size: 16 18
    image-source: /images/heldsmachine/dungeontoken
    anchors.top: devoted.top
