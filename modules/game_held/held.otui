  UIButton
    image-source: /images/heldsmachine/closebutton
    size: 28 20
    image-clip: 0 0 28 20
    anchors.top: parent.top
    anchors.right: parent.right
    opacity: 0.9
    margin-top: 20
    margin-right: 13
    @onClick: modules.HeldItems.hide()

    $hover:
      image-clip: 0 20 28 20
      opacity: 1.0

    $pressed:
      image-clip: 0 40 28 20
      opacity: 1.0